                plotStack.myPlot.bSeries.Visible = true;
                button2.Text = "Hide g(x)";
              end
              ]]>
          </onClick>
        </button>
      </stack>

      <table name="slider" widths="50% 50%" grid="false">
        <row>
          <column>
            <p name="xText" style="centerText">x = 0</p>
          </column>
          <column>
            <style>
              <widthPercentage>80%</widthPercentage>
