				<!--Problem 3-->
				<stack>
					<p style="title qNum"></p>
					<table grid="false" widths="10% 90%">
						<row>
							<column>
								<image src="curriculumThemePack\question.png" inspect="false" />
							</column>
							<column>
								<p>Determine the nightly rate required to generate maximum profit.</p>
							</column>
						</row>
					</table>
					<p style="title">ANSWER</p>
					<p name="answerA" style="collapsed">1150</p>
					<table name="table" grid="false" widths="10% 20% 60% 10%">
						<row name="row">
							<column>
								<image src="curriculumThemePack\answer.png" inspect="false" />
							</column>
							<column>
								<p><image xmlns="http://resources.qdexapps.com/schema/v1/QDocument.xsd" src="Equations/38e347a9e8ea434fad53336813904981.png" alt="\$" scale="0.0175" baseline="0" inspect="true" /></p>
							</column>
							<column name="aCol">
								<textField name="inputA" placeholder="__________" />
							</column>
							<column name="imgCol">
							</column>
						</row>
					</table>
					<stack name="solution" style="collapsed">
						<p><image xmlns="http://resources.qdexapps.com/schema/v1/QDocument.xsd" src="Equations/df6638654bb144db8e98d0926daf57cf.png" alt="\$\underline{\;1150\;}" scale="0.0175" baseline="0" inspect="true" /></p>
					</stack>
					<button content="Check Answer">
						<onClick>
							solution.Style.Visibility = "visible";
						</onClick>
					</button>
				</stack>
				<stack name="fullSolution" style="collapsed">
				<p style="title">
					<size scale="xlarge">Full Solution</size>
				</p>
				<p>
					<b>Kyoto Hotel:</b> Let <image xmlns="http://resources.qdexapps.com/schema/v1/QDocument.xsd" src="Equations/56b1f7b51ee14207802456084bb97df1.png" alt="x=\#\;of\;changes" scale="0.0175" baseline="0.194444" inspect="false" />
				</p>

				<style>
					<counter name="decBullet" format="decimal" />
				</style>

