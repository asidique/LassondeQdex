            </data>
          </series>
          <series name="xAxisLabel"></series>
          <series name="yAxisLabel"></series>
          <series name="fx">
            <style>
              <foreground color="orange" />
            </style>
          </series>
          <series name="fx2">
            <style>
              <foreground color="orange" />
            </style>
          </series>
          <series name="asymptote" draw="lineSegments">
            <style>
              <lineThickness>3</lineThickness>
              <foreground color="red" />
            </style>
          </series>
          <series name="asymptote2" draw="lineSegments">
            <style>
