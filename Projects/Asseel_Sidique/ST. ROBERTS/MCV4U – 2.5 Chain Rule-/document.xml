              <p><image xmlns="http://resources.qdexapps.com/schema/v1/QDocument.xsd" src="Equations/4f3b7aba310642779f55f25b0c4b6178.png" alt=" f(x) = sin(x) " scale="0.0175" baseline="0.25" inspect="false" /> and <image xmlns="http://resources.qdexapps.com/schema/v1/QDocument.xsd" src="Equations/5f760949e75c404aa549885b7da02dfc.png" alt=" g(x) = 2x " scale="0.0175" baseline="0.25" inspect="false" /></p>
              <p><image xmlns="http://resources.qdexapps.com/schema/v1/QDocument.xsd" src="Equations/9b037f3ed34e451e919c3910380519c2.png" alt=" f'(x) =  cos(x) " scale="0.0175" baseline="0.25" inspect="false" /> and <image xmlns="http://resources.qdexapps.com/schema/v1/QDocument.xsd" src="Equations/588305646ce6452a9fd6da2e448e2870.png" alt=" g'(x) = 2 " scale="0.0175" baseline="0.25" inspect="false" /></p>
