    <stack style="contentBG">
      <p style="subSectionHeading">Example 2</p>
      <table widths="10% 90%" grid="false">
        <row>
          <column>
            <image src="resources\question.png" inspect="false" />
          </column>
          <column>
            <p><image xmlns="http://resources.qdexapps.com/schema/v1/QDocument.xsd" src="Equations/69467fa6980849d6ba94326a4013093a.png" alt=" F(x) = sin(2x), " scale="0.0175" baseline="0.25" inspect="false" /> find <image xmlns="http://resources.qdexapps.com/schema/v1/QDocument.xsd" src="Equations/fd4f8735804743e39d067e92d72e1f7f.png" alt=" F'(x): " scale="0.0175" baseline="0.25" inspect="false" /></p>
