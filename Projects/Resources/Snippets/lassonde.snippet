  <!--A Checkbox Question Stack-->
  <CodeSnippet Format="1.0.0">
    <Header>
      <Title>Checkbox Question Stack</Title>
      <Description>A checkbox question stack.</Description>
      <Author>Jason Wang</Author>
      <Shortcut>las_cstack</Shortcut>
    </Header>
    <Snippet>
      <Declarations>
        <Literal>
          <ID>answers</ID>
          <ToolTip>Replace with the correct answers table</ToolTip>
          <Default>{true, false, false, true}</Default>
        </Literal>
        <Literal>
          <ID>stack_title</ID>
          <ToolTip>Replace with stack title</ToolTip>
          <Default>Question 1</Default>
        </Literal>
        <Literal>
          <ID>stack_question</ID>
          <ToolTip>Replace with checkbox question</ToolTip>
          <Default>Replace with your checkbox questions here!</Default>
        </Literal>
        <Literal>
          <ID>choice_1</ID>
          <ToolTip>Replace with your first choice element, use style="mcLayout" on your element to center the choice</ToolTip>
          <Default><![CDATA[<p style="mcLayout">1</p>]]></Default>
        </Literal>
        <Literal>
          <ID>choice_2</ID>
          <ToolTip>Replace with your second choice element</ToolTip>
          <Default><![CDATA[<p style="mcLayout">2</p>]]></Default>
        </Literal>
        <Literal>
          <ID>choice_3</ID>
          <ToolTip>Replace with your third choice element</ToolTip>
          <Default><![CDATA[<p style="mcLayout">3</p>]]></Default>
        </Literal>
        <Literal>
          <ID>choice_4</ID>
          <ToolTip>Replace with your fourth choice element</ToolTip>
          <Default><![CDATA[<p style="mcLayout">4</p>]]></Default>
        </Literal>
      </Declarations>
      <Code Language="XML">
        <![CDATA[
    <stack style="mcStackLayout">
      <script>
        local answers = $answers$
        local cqController = CQController:new({checkboxes={table:Child(2):Child(1).checkbox, table:Child(2):Child(2).checkbox, table:Child(4):Child(1).checkbox, table:Child(4):Child(2).checkbox}, checkBtn=checkBtn, answers=answers})
      </script>
			<p style="subSectionHeading">$stack_title$</p>
			<table widths="20% 80%" grid="false">
				<row>
					<column>
						<image src="resources/question.png" inspect="false" />
					</column>
					<column>
						<p style="justifiedText">$stack_question$</p>
					</column>
				</row>
			</table>
			<table name="table" grid="false" widths="50% 50%">
				<row>
					<column>
            $choice_1$
					</column>
					<column>
						$choice_2$
					</column>
				</row>
				<row>
					<column>
						<button name="checkbox" style="checkbox_style" content=" ">
              <onClick>
                cqController:toggleCheckbox(1)
              </onClick>
            </button>
					</column>
					<column>
						<button name="checkbox" style="checkbox_style" content=" ">
              <onClick>
                cqController:toggleCheckbox(2)
              </onClick>
            </button>
					</column>
				</row>
				<row>
					<column>
						$choice_3$
					</column>
					<column>
						$choice_4$
					</column>
				</row>
				<row>
					<column>
						<button name="checkbox" style="checkbox_style" content=" ">
              <onClick>
                cqController:toggleCheckbox(3)
              </onClick>
            </button>
					</column>
					<column>
						<button name="checkbox" style="checkbox_style" content=" ">
              <onClick>
                cqController:toggleCheckbox(4)
              </onClick>
            </button>
					</column>
				</row>
			</table>
			<button name="checkBtn" content="Check Answer">
				<onClick>
          cqController:checkAnswer()
        </onClick>
			</button>
		</stack>
        ]]>
      </Code>
    </Snippet>
  </CodeSnippet>
</CodeSnippets>