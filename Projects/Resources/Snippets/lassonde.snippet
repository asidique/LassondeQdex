      <Author>Jason Wang</Author>
      <Shortcut>las_checkbox</Shortcut>
    </Header>
    <Snippet>
      <Declarations>
        <Literal>
          <ID>checkbox_question_controller</ID>
          <ToolTip>Replace with the CQController variable name</ToolTip>
          <Default>cqController</Default>
        </Literal>
        <Literal>
          <ID>button_index</ID>
          <ToolTip>Replace with current button index (index starts from 1)</ToolTip>
          <Default>index</Default>
        </Literal>
      </Declarations>
